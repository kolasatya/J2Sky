    </style>
    <style name="hzlistlrleyout">
        <item name="android:background">@drawable/lrly_hz_round_corners</item>
    </style>
    <style name="linear_login_fb">
        <item name="android:background">@drawable/lrly_login_fb_round_corners</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="linear_login_google">
        <item name="android:background">@drawable/lrly_login_google_round_corners</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="txt_login_white">
        <item name="android:textColor">#E9F7DE</item>

    </style>

    <style name="btnregister">
        <item name="android:textColor">#FFFFFF</item>
