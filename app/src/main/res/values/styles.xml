<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- the theme applied to the application or activity -->
    <style name="CustomActionBarTheme"
        parent="@android:style/Theme.Holo.Light">


        <item name="android:actionBarStyle">@style/MyActionBar</item>

    </style>
    <style name="normaltheme" parent="@android:style/Theme.Holo.Light">
    <item name="android:textColor">#5e5e5e</item>
    <item name="android:textSize">25sp</item>
    <item name="android:textStyle">bold</item>
    </style>
    <!-- ActionBar styles -->
    <style name="MyActionBar"
        parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">

        <item name="android:backgroundStacked">#1976d2</item>
        <item name="android:backgroundSplit">#1976d2</item>
        <item name="android:background">#1976d2</item>
        <item name="android:logo">@android:color/transparent</item>

        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>

    </style>

    <style name="MyActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#FFFFFF</item>

    </style>
    <style name="Actionfooter">

        <item name="android:background">#1795A4</item>
    </style>


    <style name="frmbackgroud" >

        <item name="android:background">#1695A3</item>

    </style>

    <style name="TxtBlackFont" parent="@android:style/TextAppearance.Medium" >

        <item name="android:textColor">#3f3f3f</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">bold</item>

    </style>
    <style name="TxtTitleFont" parent="@android:style/TextAppearance.Medium" >
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#9A2EFE</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <!-- <item name="android:background">#FFFFFF</item> -->
    </style>

    <style name="lineargreyout">
